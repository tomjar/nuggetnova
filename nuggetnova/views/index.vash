@html.extend('layout', function(model) {
  @html.block('content', function(model) {
    @model.posts.forEach(function(item){
      <div class="row">
      <div class="col">
      <h6 class="card-subtitle text-muted" style="font-size:xx-small;"><i>@item.createtimestamp</i></h6>
      <h5 class="card-title"><a href='post/view/@item.postid'>@item.title</a></h5>
                <p>
          @item.description
          </p>
      </div>
      </div>
    })
  });
})
