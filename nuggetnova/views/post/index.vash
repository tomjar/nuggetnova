@html.extend('layout', function(model) {
  @html.block('content', function(model) {
    <h1>@model.title</h1>
        <table class="table text-white bg-dark">
      <tr>
        <th>title</th>
        <th></th>
      </tr>
    @model.posts.forEach(function(item){
      <tr>
	      <td>@item.title</td>
        <td><a href='edit/@item.postid'>edit</a></td>
      </tr>
    })
    </table>
  })
})