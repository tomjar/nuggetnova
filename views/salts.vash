@html.extend('layout', function(model) {
  @html.block('content', function(model) {
    <h1>@model.title</h1>
        <table class="table">
            <tr>
                <th>salt</th>
            </tr>
            @model.salts.forEach(function(item){
            <tr>
                <td>@item</td>
            </tr>
            })
        </table>
  })
})